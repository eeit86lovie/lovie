<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kidscodetw.eeit.entity.custservice.CustServiceBean" table="CustService" >
        
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="memberId" column="memberId" />
        <property name="catagory" column="catagory" />
        <property name="message" column="message" />
        <property name="replyMessage" column="replyMessage" />
        <property name="status" column="status" />
        
    
        
    </class>
</hibernate-mapping>