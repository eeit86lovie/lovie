<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kidscodetw.eeit.entity.MovieBean" table="Movie" >
        
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        
        <property name="name" column="name" />
        <property name="rank" column="rank" />
        <property name="startTime" column="startTime" />
        <property name="filmLength" column="filmLength" />
        <property name="intro" column="intro" />
        <property name="photoUrl" column="photoUrl" />
        <property name="photo" column="photo" type="binary" />
        <property name="trailer" column="trailer" />
        <property name="online" column="online" />
        
    </class>
</hibernate-mapping>