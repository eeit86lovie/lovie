<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kidscodetw.eeit.entity.ShowtimeBean" table="Showtime" >
        
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        
        <property name="movieName" column="movieName" />
        <property name="theaterName" column="theaterName" />
        <property name="showtimeDate" column="showtimeDate" />
        <property name="showtimeTime" column="showtimeTime" />
        <property name="online" column="online" />

    </class>
</hibernate-mapping>